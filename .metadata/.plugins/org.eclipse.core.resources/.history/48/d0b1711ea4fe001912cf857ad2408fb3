<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.PerticipateEventMapper">

	<select id="joinSave" resultType="com.example.domain.PerticipateEvent">
		insert into event(id,username,participate)values(#{id},#{username},#{join});
	</select>

	<select id="findJoin" resultType="com.example.domain.PerticipateEvent">
		select * from event where id = #{id} and username = #{username} and participate = #{partcipate}
	</select>

</mapper>