package com.example.mapper;

import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Param;

@Mapper
public interface PerticipateEventMapper {

	void joinSave(@Param("id")int id,@Param("username")String username,@Param("join")String join);

	boolean findJoin(@Param("id")int id,@Param("username")String username,@Param("join")String join);

}
